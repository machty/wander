{
  "browser": true,
  "jquery": true,
  "node": true,
  "esnext": true,

  "camelcase": true,
  "eqeqeq": true,
  "indent": 2,
  "latedef": true,
  "maxlen": 90,
  "newcap": true,
  "quotmark": "single",
  "strict": false,
  "undef": true,
  "unused": true,
  "validthis": true,

  "predef": [
    "document",
    "window",
    "-Promise",
    "moment",
    "$",
    "console",
    "visit",
    "fillIn",
    "click",
    "andThen"
  ]
}
